    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.1",
    "prettier": "^2.8.3",
    "ts-jest": "29.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
